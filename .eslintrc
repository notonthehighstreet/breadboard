{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eqeqeq": 2,
    "curly": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-console": 2,
    "camelcase": 2,
    "consistent-this": [2, "self"],
    "brace-style": [2, "stroustrup"],
    "eol-last": 2,
    "comma-spacing": 2,
    "space-before-function-paren": [2, "never"],
    "indent": [2, 2],
    "no-shadow": 2,
    "space-before-blocks": 2,
    "new-cap": 0,
    "semi": [2, "always"]
  }
}
